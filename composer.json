{
    "name": "gpupo/common-schema",
    "description": "Common schema",
    "homepage": "https://opensource.gpupo.com/common-schema/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["sphinx", "schema", "xml", "xmlpipe2", "order"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Gpupo\\CommonSchema\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\CommonSchema\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "gpupo/common": "^2.7",
        "symfony/console": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/yaml": "^4.0",
        "gpupo/common-sdk": "^4.3",
        "doctrine/orm": "^2.6"
    }
}
